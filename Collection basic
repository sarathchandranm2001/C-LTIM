using System;
using System.Collections;

class Programs
{
    static void Main(string[] args)
    {
        Console.WriteLine("Hello, World!");
        Demo_ArrayList(); // Call the method
    }

    static void Demo_ArrayList()
    {
        ArrayList obj = new ArrayList();
        obj.Add("America");
        obj.Add("India");
        obj.Add("UK");

        // Insert an item at index 2
        obj.Insert(2, "Germany");

        Console.WriteLine("Capacity: " + obj.Capacity);
        Console.WriteLine("Count: " + obj.Count); // Total number of items

        Console.WriteLine("Contents of ArrayList:");
        foreach (var item in obj)
        {
            Console.WriteLine(item);
        }
    }
}
